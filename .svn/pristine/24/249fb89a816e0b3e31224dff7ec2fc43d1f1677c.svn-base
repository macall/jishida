{// 引入标签库 }
<tagLib name="html" />
<include file="Public:header" />
<div class="main">
<div class="main_title">{%ADD} <a href="{:u("ServiceLevel/index")}" class="back_list">{%BACK_LIST}</a></div>
<div class="blank5"></div>
<form name="edit" action="__APP__" method="post" enctype="multipart/form-data">
<table class="form" cellpadding=0 cellspacing=0>
	<tr>
		<td colspan=2 class="topTd"></td>
	</tr>
	<tr>
		<td class="item_title">星级名称:</td>
		<td class="item_input"><input type="text" class="textbox require" name="levelname" /></td>
	</tr>
        <tr>
		<td class="item_title">级别:</td>
		<td class="item_input"><input type="text" class="textbox require" name="level" /></td>
	</tr>
<!--	<tr>
		<td class="item_title">{%USER_GROUP_SCORE}:</td>
		<td class="item_input"><input type="text" class="textbox" name="score" /></td>
	</tr>
	<tr>
		<td class="item_title">{%DISCOUNT}:</td>
		<td class="item_input"><input type="text" class="textbox" name="discount" value="1" />
		<span class='tip_span'>[{%DISCOUNT_TIP}]</span>
		</td>
	</tr>	-->
	
	<tr>
		<td class="item_title"></td>
		<td class="item_input">
			<!--隐藏元素-->
			<input type="hidden" name="{:conf("VAR_MODULE")}" value="ServiceLevel" />
			<input type="hidden" name="{:conf("VAR_ACTION")}" value="insert" />
			<!--隐藏元素-->
			<input type="submit" class="button" value="{%ADD}" />
			<input type="reset" class="button" value="{%RESET}" />
		</td>
	</tr>
	<tr>
		<td colspan=2 class="bottomTd"></td>
	</tr>
</table>	 
</form>
</div>
<include file="Public:footer" />