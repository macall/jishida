/* CSS Document */
.order_data_box{width:100%; overflow: hidden;}
.order_data_box .top_data{position: relative}
.order_data_box .top_data img{width:100%;  display: block;}
.order_data_box .top_data ul{overflow: hidden}
.order_data_box .top_data ul li{position: absolute;box-shadow:none;}
.order_data_box .top_data ul li.return{background:url("../images/huanhui1.png") no-repeat; top:35%;left:10px; z-index: 99; background-size:15px;border:0;}
.order_data_box .top_data ul li.return a{padding-left:20px;color:#fff; font-size:13px;}
.order_data_box .top_data ul li.phone{position: absolute; text-align: center; top:35%; color:#fff; z-index: 1;width: 100%;background:none;border:0; }
.order_data_box .top_data ul li.phone img{width:49px;height:49px;display:inline;border-radius:49px;}
.order_data_box .top_data ul li.phone i{ background-size:contain;font-size:20px;}
