.event_submit_row{
	padding:10px 0;
}
.event_submit_row span{
	vertical-align: middle;
	line-height: 44px;
	width: 65px;
	text-align: right;
}
.f-input{
	width:100%;
	height:42px;
	border:1px solid #efefef;
	background:#f5f5f5;
	color:#bcbcbc;
	font-size:18px;
	line-height:46px;
	padding:0 20px;
	box-sizing:border-box;
	-moz-box-sizing:border-box; /* Firefox */
	-webkit-box-sizing:border-box; /* Safari */
}


.activity_d_block{
	margin:10px;
	padding:10px;
	border:1px solid #d5d5d5; 
	background:@box_color;
	color:#727272;
}

.goods_intro h4{
	font-size:16px;
}
.goods_intro p{
	font-size:13px;
	line-height:20px;
	}

.fe8d89{
	color:#fe8d89;
}



.prompt_block h4{
	font-size:22px;
}

.prompt_block p{

	line-height:24px;
	font-size:12px;
	}
.prompt_block p img{
	width:100%;
}
.prompt_block til{
	font-size:17px;
	color:#484848;
}
	
.entry_line {
	display:-webkit-box;
	padding:10px 0;
}	
	
	
.entry_line span{
	display:inline-block;
	height:48px;
	line-height:48px;
	
	color:#737373;
	text-align:right;
	padding-right:10px;
}
	
	
.entry_line .text_block{
	height:48px;
	padding-right:50px;
	-webkit-box-flex:1;
}	
	
.entry_line .text_block input{
	width:100%;
	height:46px;
	border:1px solid #efefef;
	background:#f5f5f5;
	color:#bcbcbc;
	font-size:18px;
	line-height:46px;
	padding:0 20px;
}	
	
.entry_line_s{
	margin-top: 10px;
	height: 68px;
	padding-right: 10px;
}
	
.entry_line_s .entry_select{
	display:inline-block;
	width:280px;
}	

.f_left {
	float:left;
}
	
.f_right{
	float:right;
}	

.entry_line_s .entry_select .select{
	display:inline-block;
	position:relative;
	border:1px solid #d5d5d5;
	background: #fff;
	left:-3px;
	height: 50px;
	width:130px;
	
}
.entry_line_s .entry_select .select .select_txt{
	display:inline-block;
	width:90px;
	height:50px;
	line-height:50px;
	text-align:center;

}
.entry_line_s .entry_select .select .back_pic{
	display:inline-block;
	width:40px;
	height:50px;
	line-height:50px;
	text-align:center;
	color:#575757;
	position:absolute;
	top:0px;
	border-left:1px solid #d5d5d5;
	
}
.entry_line_s .entry_select .select ul{
	position:absolute;
	top:51px;
	left:-1px;
	border:1px solid #d5d5d5;
	background:#fff;
	border-top:none;
	display:none;
	z-index: 10;
    
}

.entry_line_s:last-child{
	
	height:0px;
}

.entry_line_s:last-child .entry_select {
float: right;
margin-top: -75px;
}

.entry_line_s .entry_select .select ul li{
	border-bottom:1px solid #d5d5d5;
	width:130px;
	margin-bottom:-1px;
	line-height: 30px;

text-align: center;
	
}
.entry_line_s .entry_select span{
	display:inline-block;
	height:48px;
	line-height:48px;

	color:#737373;

	text-align:right;
	padding-right:10px;
	float: left;
}

.entry_line_s .entry_select select{
	width:65px;
	height:50px;
	border:1px solid #d5d5d5;
	-webkit-appearance:menulist;
     -moz-appearance:menulist;
	
}

.critical_list{
	border:1px solid #d5d5d5;
	margin:10px;
	background:#fff;
}

.critical_list ul li{
	border-bottom:1px dashed #d5d5d5;
	padding:13px;
	padding-bottom:3px;
	position:relative;
	margin-bottom: -1px;
}

.critical_list ul li .pic{
	position:absolute;
}

.critical_list ul li .txt{
	margin-left:73px;
	min-height: 73px;
}
.critical_list ul li .txt p{
	color: #818181;
line-height: 20px;
min-height: 40px;
font-size:12px;
}
.critical_list ul li .txt .name_time{ 	
   position: relative;
	padding-bottom: 5px;
	line-height: 20px;
	min-height:20px;
}
.critical_list ul li .txt .name_time h5{
	color:#575757;
	font-size:15px;
	padding-right: 75px;
    overflow: hidden;
}
.critical_list ul li .txt .name_time span{
	position:absolute;
	right:0px;
	top:0px;
	color:#5c5c5c;
	font-size:12px;
}

