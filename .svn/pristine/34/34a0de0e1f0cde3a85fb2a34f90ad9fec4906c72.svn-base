.deal-terms ul li{list-style-type:disc;}
.deal-advice ul{padding:0px;margin-left:10px;}
/*.deal-advice ul  li {padding:25px 0px; background:url(../images/tuan_next.png) no-repeat 98.1% center ;border-bottom: 1px dashed rgba(0,0,0,.1);}*/
.deal-advice a.advice-buy{display:block;widht:100%;}
.sc_ori{ font-size:22px; font-weight:blod; color:#FE4700; line-height:20px;}
.starsBarBig{ background: url(../images/star_bar.png) no-repeat 0 -79px;position: relative;}
.starsBarBig .s{display:block; height:20px; background: url(../images/star_bar.png) no-repeat 0 -48px;}
#J_post_cart:hover{cursor:pointer;text-decoration:none;}
.box-style h2 .icon-fonts{padding-top:0px}
.box-style .more_next {
font-family: Test;
content: 'F';
position: absolute;
right: 3px;
top: 4px;
width: 1em;
margin-right: .4em;
text-align: center;
opacity: .7;
line-height: 1em;
font-size: 14px;
}
.comment-acount .more_next_count{
font-family: Test;
content: 'F';
position: absolute;
right: -1px;
top: 7px;
width: 1em;
margin-right: .4em;
text-align: center;
opacity: .7;
line-height: 1em;
font-size: 14px;
color: #D5D5D5;
}
.advice-buy {
padding: 12px 25px 12px 8px;
display: block;
color:#676767;
}
.buy_next{
font-family: Test;
content: 'F';
position: absolute;
right: 7px;
top: 12px;
color: #D5D5D5;
height:20px;
}
