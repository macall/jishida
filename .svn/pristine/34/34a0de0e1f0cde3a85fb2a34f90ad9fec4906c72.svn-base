.deal-detail h2 .icon-fonts{background:url(../images/tuan_taocan.png) no-repeat 0 center;display: inline;padding: 19px ;}
.deal-terms  h2 .icon-fonts{background:url(../images/tuan_xuzhi.png) no-repeat 0 center;display: inline;padding: 19px ;}
