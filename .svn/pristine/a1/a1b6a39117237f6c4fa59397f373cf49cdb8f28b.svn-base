.nav_item .icon_Review{background-position: 0px -261px;}
.nav_item .icon_Project{background-position: 0px -301px;}
.nav_item .icon_Bills{background-position: 0px -339px;}
.nav_item .icon_Order{background-position: 0px -376px;}
.nav_item .icon_Location{background-position: 0px -415px;}


.right_box .content{min-height: 620px;margin: 0 0;_margin: 0 0;padding: 0 0 30px 0;border-radius: 0 2px 2px 0;background: #fff;zoom: 1;width:808px;overflow:hidden;border:1px solid #e7e7e7;}
.right_box .content_hastab{position: relative;padding: 45px 20px 50px;width: 769px;}
.standard_tab{border-bottom: 2px solid @main_color;font-size:14px;}
.standard_tab li{float: left;margin-right: 6px;font-weight: 700;border: 1px solid #dedede;border-bottom: none;border-radius: 3px 3px 0 0;color: #666;background-color: #f5f5f5;}
.standard_tab li a{display: inline-block;color: #333;text-decoration: none;padding: 0 20px;line-height: 34px;min-width: 50px;text-align: center;zoom: 1;}
