/*提示框*/
.common_tip{margin-top: 2px;padding: 10px;border: 1px solid #fbf0d5;background-color: #fffbf2;font-size:14px;}
.common_tip h5 {margin-top: 10px;font-size: 100%;}
.common_tip ul {margin-top:10px;}
.common_tip ul li {list-style-type: disc;margin-bottom: 5px;list-style-position: inside;}

/*上传图片集框*/
.upload_btn_box{width:110px;display: inline-block;*zoom:1;*display:inline;}
.pub_upload_img_box{display: inline-block;*zoom:1;*display:inline;}
.pub_upload_img_box span{display: inline-block;*zoom:1;*display:inline;width: 50px;height: 50px;overflow:hidden;float: left;margin-right: 5px;position: relative;padding: 1px;border: #ddd 1px solid;}
.pub_upload_img_box span img{width:50px;}
.pub_upload_img_box span a:hover {display: block;width: 50px;height: 50px;position: absolute;left: 1px;bottom: 1px;background: url(../images/css_bg/del_img.png) no-repeat 0 bottom;z-index: 10;filter: alpha(opacity=70);-moz-opacity: 0.7;opacity: 0.7;-moz-transition: all 0.2s;-webkit-transition: all 0.2s;transition: all 0.2s;}
.pub_upload_img_box span a {display: block;width: 50px;height: 50px;position: absolute;left: 1px;bottom: 1px;background: url(../images/css_bg/del_img.png) no-repeat 0 bottom;z-index: 10;filter: alpha(opacity=0);-moz-opacity: 0;opacity: 0;-moz-transition: all 0.2s;-webkit-transition: all 0.2s;transition: all 0.2s;}
.pub_upload_img_box span div.loader {width: 50px;height: 50px;position: absolute;left: 1px;top: 1px;z-index: 0;display: inline-block;background: url(../images/css_bg/loading.gif) no-repeat center center;}

