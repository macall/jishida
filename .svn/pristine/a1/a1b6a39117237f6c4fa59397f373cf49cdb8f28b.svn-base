@CHARSET "UTF-8";
/* 公共样式 */
body{background:@body_color;   word-break:break-all;   word-wrap:break-word;  font-size:12px; font-family: Arial,"宋体",Verdana; line-height:150%; margin:0px; padding:0px; color:@font_color; }
input{ font-family:Arial;}
div{margin:0 auto; padding:0;}
h1,h2,h3,h4,h5,h6,ul,li,dl,dt,dd,form,img,p{margin:0; padding:0; border:none; list-style-type:none;}
table{border-collapse: collapse;border-spacing: 0;}
.wrap_full{ width:990px; height:auto;} /*主体容器*/


.f_l{float:left;}
.f_r{float:right;}
.tl{ text-align:left;}
.tc{ text-align:center;}
.tr{ text-align:right;}

.hide{display:none;}
/*自动高对齐*/
.clearfix:after{
content:"."; display:block; height:0; clear:both;
visibility:hidden;
}
*html .clearfix{
 height:1%;
}
*+html .clearfix{
 height:1%;
}
.cf {
	zoom: 1
}
.cf:after {
	content: '';
	display: block;
	clear: both;
	height: 0;
	overflow: hidden;
	visibility: hidden;
}
/*全局链接样式*/
a{ color:@font_color; text-decoration:none; }
a:hover{ color:@font_hover; text-decoration:none; }

/*英文强制换行*/
.word{word-break:break-all;}

/*页面留白*/
.clear{height:0px; line-height:0px; clear:both; visibility:hidden;}
.blank{height:10px; line-height:10px; clear:both; visibility:hidden;}
.blank5{height:5px; line-height:5px; clear:both; visibility:hidden;}
.blank10{height:10px; line-height:10px; clear:both; visibility:hidden;}
.blank15{height:15px; line-height:15px; clear:both; visibility:hidden;}
.blank20{ height:20px; line-height:20px; clear:both; visibility:hidden; }
/*阴影*/
.shadow{
-moz-box-shadow: 2px 2px 10px @shadow_color;/*firefox*/
-webkit-box-shadow: 2px 2px 10px @shadow_color;/*safari或chrome*/
box-shadow:2px 2px 10px @shadow_color;/*opera或ie9*/
}
/*向下三角形*/
.triangle{
	height: 0;
width: 0;
position: absolute;
right: 8px;
top: 16px;
overflow: hidden;
border-left: 4px solid transparent;
border-right: 4px solid transparent;
border-top: 5px solid transparent;
border-top-color: #fff;
_border-left-color: tomato;
_border-right-color: tomato;
_filter: chroma(color=tomato);
}
.triangle-top{
	height: 0;
	width: 0;
	position: absolute;
	overflow: hidden;
	border-left: 4px dotted transparent;
	border-right: 4px dotted transparent;
	border-bottom: 5px solid transparent;
	border-bottom-color: #FFF;
}

/*滚动到头部*/
#go_top{ background:url(../images/css_bg/gotop.gif) 12px 12px no-repeat @main_color;  cursor:pointer;
	position:absolute;
	top:0px;
	right:0px;
	width:60px;
	height:60px; 
	margin-right:23px;
	margin-bottom:20px; 
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	border-radius:3px;
	display:none;
	z-index:60;
}

/* iconfont */
@font-face {font-family: "iconfont";
  src: url('../images/iconfont/iconfont.eot?@rand'); /* IE9*/
  src: url('../images/iconfont/iconfont.eot?#iefix&@rand') format('embedded-opentype'), /* IE6-IE8 */
  url('../images/iconfont/iconfont.woff?@rand') format('woff'), /* chrome、firefox */
  url('../images/iconfont/iconfont.ttf?@rand') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('../images/iconfont/iconfont.svg#iconfont?@rand') format('svg'); /* iOS 4.1- */
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

/* 关于分页 */
.pages{ text-align:center;color:@font_color; height:24px; line-height:24px;}
.pages a{ display:inline-block; padding:0px 3px; height:22px; line-height:22px; background:#fff; color:@main_color; border:1px solid @main_color; text-decoration:none;}
.pages a:hover{ display:inline-block;padding:0px 3px; height:22px; line-height:22px;  color:#fff; background:@main_color;border:1px solid @main_color;  text-decoration:none;}
.pages .current{  display:inline-block; padding:0px 5px; height:22px; line-height:22px;  color:#fff; background:@main_color;border:1px solid @main_color}


/*商户模块通用样式*/
.icon{background:url("../images/css_bg/biz_icon.gif") no-repeat}
.icon-kefu{background-position:0 0}
.icon-email{background-position:0 -88px}


/*头部样式*/
.biz_header{background:@main_color;height:67px;border-bottom:3px solid @main_color;z-index: 3;position:relative;}
.biz_header .head_box{position:relative;height:100%;z-index:2;zoom:1;}
.head_box .logo{padding-top:24px;}
.head_box .logo h1{font-size:24px;color:#fff;}
.head_box .head_nav{position:absolute;right:0;bottom:4px;}
.head_nav .option{float:left;position:relative;border:1px solid transparent;_border-color:@main_color;text-align:center;zoom:1;padding:1px 0;}
.head_nav .option a{padding:0 10px; display:inline-block;color:#fff;zoom:1;}
.head_nav .option .vertical-bar{position:absolute;width:1px;height:10px;background:@dark_color;top:4px;right:-2px;_right:-3px;}
.head_nav .option-qrcode {position:relative;top: -5px;width: 168px;height: 21px;padding-top:5px;padding-right: 5px;padding-bottom: 5px;border: 1px solid transparent;_border: none;}
.head_nav .option-qrcode a{display: inline-block;text-decoration: none;font-size: 12px;color: #fff;text-shadow: @main_color 1px 1px;zoom: 1;}
.head_nav .option-qrcode .triangle{right: 14px;top: 12px;_top: 11px;}
.head_nav .option-qrcode .qrcode-box{position: absolute;width: 173px;background: #fff;border: 1px solid #e7e7e7;top: 32px;left: -1px;display:none;}
.head_nav .option-qrcode .qrcode-box a{display: block;margin: 0 auto;}
.head_nav .option-qrcode .qrcode-box .qrcode-text{color:@main_color;}
.head_nav .option-qrcode.curr{background-color:@dark_color;border-radius: 3px;text-decoration: none;}
.head_nav .option-qrcode.curr .vertical-bar{display:none;}
.head_nav .option-qrcode.curr .qrcode-box{display:block;}
.head_nav .option-qrcode.curr .triangle{border-bottom: 5px solid transparent;border-bottom-color: #FFF;border-top:0;}
/*底部样式*/
.biz_footer{border-top:2px solid @main_color;background:#fff}
.biz_footer .footer_box{text-align:center;padding:35px 0 50px; 0}
.footer_box .con_item{display:inline-block; _zoom:1;*display:inline;border-left:1px dotted @border_color;width:160px;height:70px;padding:0 30px 0 20px;color:#666;zoom:1;vertical-align:top;text-align:left;}
.footer_box .con_item h3{color:#000;font-size:16px;}
.footer_box .con_item .content{font-weight: 700;}
.footer_box .con_item .content .time-limit{font-size:12px;font-weight:400}
.footer_box .icon{display: inline-block;margin-right: 10px;width: 60px;height:60px;border-radius: 5px;color: #fff;background-color: @main_color;}

/*头部错误提示*/
.sysmsg_success {border: 1px solid #d1edad;background: #f3fee5;}
.sysmsg_error{border: 1px solid #ffddda;background: #fff4f3;}

.msg_tip{color: #404040;position:relative;padding:0 30px;display:none;margin-top: 8px;}
.msg_tip .msg_content{padding: 9px 10px;font-size: 14px;color: #333;}
.msg_tip .status,.msg_tip .close{display:block; background:url("../images/css_bg/biz_icon2.gif") no-repeat;}
.msg_tip .s_error{position:absolute;left:15px;top:10px;background-position:-41px 1px;width:15px;height:15px;}
.msg_tip .s_success{position:absolute;left:15px;top:10px;background-position:-41px -29px;width:15px;height:15px;}
.msg_tip .close{display:block;position:absolute;background-position:-55px 1px;right:12px;top:12px;width:10px;height:10px;overflow:hidden;}
.msg_tip .close{ transition:All 0.3s ease-in-out;-webkit-transition:All 0.3s ease-in-out;-moz-transition:All 0.3s ease-in-out;-o-transition:All 0.3s ease-in-out;}
.msg_tip .close:hover {transform:rotate(90deg);-webkit-transform:rotate(90deg);-moz-transform:rotate(90deg);-o-transform:rotate(90deg);-ms-transform:rotate(90deg);}


