header .right-box a span {
display: block;
background-size: auto 29px;
height: 25px;
padding: 4px 8px 0 8;
margin-right: -0px;
text-shadow: 0 0 1px #000;
background-position: right 0;
font-family: Test;
font-size: 14px;
background: -webkit-linear-gradient(top,#EB2555,#A04E65);
border: 1px solid #B80F38;
}

ul, ol {
padding-left: 0;
list-style-type: none;
}
.dl {
display: block;
min-height: 78px;
position: relative;
padding: 10px 8px 10px 140px;
border-top: 1px #F1F1F1 solid;
}
.dl>img {
width: 122px;
left: 8px;
position: absolute;
padding: 1px;
border: 1px #DBE5E7 solid;
}
.dl-brand {
font-size: 14px;
line-height: 15px;
color: #000;
font-weight: 700;
margin-top: 5px;
}
.break-word {
word-wrap: break-word;
overflow: hidden;
}
.dl-title {
line-height: 16px;
color: #999;
font-size: 13px;
margin-top: 4px;
}
.dl-price {
font-size: 12px;
color: #AAA;
position: relative;
}
.dl-price>strong {
font-size: 18px;
color: #a11d58;
}
strong {
font-weight: 700;
}
.dl-price>span {
position: absolute;
right: 0;
bottom: 1px;
}
Inherited from li.dl-price
.dl-price {
font-size: 12px;
color: #AAA;
}

