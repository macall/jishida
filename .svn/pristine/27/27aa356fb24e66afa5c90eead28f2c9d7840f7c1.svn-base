.daren_recommend_box{ position:relative;background:@box_color;border: @main_color 3px solid;border-radius: 5px; }
.darenshow_tag{ position:absolute; background:url(../images/daren/darenshow_tag.png) no-repeat; width:56px; height:57px; left:-3px; top:-3px;
_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../images/daren/darenshow_tag.png', sizingMethod='scale');
_background-image:none; }
.daren_recommend_box .m{width:970px; padding:25px 25px 20px 25px; }

.daren_list{ width:690px; float:left;z-index:10;}
.daren_submit{ background:#f3f3f3; width:250px; height:146px; float:left;}
.daren_submit a{ display:block; margin:15px auto 0px auto; width:210px; height:110px;}
.daren_list a{ float:left; margin:0px 1px 1px 0px; }
.tp_content{ height:30px;}

.show-nav {
  background: @box_color;
  height: 30px;
  line-height: 30px;
  margin: 0 auto;
  border-bottom: 2px solid  @main_color;
  width: 990px;
}

.show-nav li {
  display: inline;
  float: left;
  text-align: center;
  width: 120px;
}

.show-nav li.on a, .show-nav li.on a:hover {
    background: @main_color;
    color: #FFFFFF;
    font-weight: bold;
}
.show-nav li.first a, .show-nav li.first a:hover {
    margin-right: 1px;
    width: 120px;
}

.show-nav li a, .show-nav li a:hover {
    color: #333333;
    display: block;
    font-size: 14px;
    text-decoration: none;
    width: 120px;
}
.show-nav li.last {
  width: 119px;
}


.show-nav li.last a, .show-nav li.last a:hover {
    width: 119px;
}

.show-nav li a, .show-nav li a:hover {
    color: #333333;
    display: block;
    font-size: 14px;
    text-decoration: none;
    width: 119px;
}


.fix-nav{	
	position:fixed;
	top:0;
	left:0;
	width:100%;	
	text-align:center;	
	height:32px;
	border-bottom:1px solid #c8cac6;
	background:rgba(250,250,250,.8);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#ccffffff,endColorstr=#ccffffff);
	padding-top:8px;
	-webkit-box-shadow:#ccc 0 1px 2px;-moz-box-shadow:#ccc 0 1px 2px;box-shadow:#ccc 0 1px 2px;
	_position:absolute;	
}



.cate_title{  border-bottom: 2px solid  @main_color; width:990px; height:30px; line-height:28px; font-weight:bolder; font-size:14px; }
.rec_topic_item{ display:inline-block; width:460px; height:280px; overflow:hidden; float:left; margin-bottom:20px;}
.r{ margin-left:70px;}
.fav_row{ background:url(../images/daren/daren_icon_bg.png) no-repeat -60px 3px; padding-left:17px; padding-right:10px;}
.fav_row span{ color:#e66e00; font-weight:bolder;}
.topic_view{ display:inline-block; border:#ccc solid 1px;}
.topic_view img{ width:458px; height:160px;}

