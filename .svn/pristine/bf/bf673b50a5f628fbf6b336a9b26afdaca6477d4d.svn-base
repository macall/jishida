}

.ft-nav ul {
font-size: 0;
text-align: center;
height: 35px;
overflow: hidden;
border-top: 1px #FFF solid;
}

.ft-nav a:after {
content: '-';
height: 28px;
width: 1px;
background: #CCC;
position: absolute;
right: 0;
top: 4px;
font-size: 0;
text-indent: -9999px;
background: -webkit-gradient(linear,0 0,0 100%,from(#FFF),to(#FFF),color-stop(0.5,#CCC));
}
.ft-nav a {
display: block;
}
.clearfix:after {
display: block;
content: '';
clear: both;
visibility: hidden;
}

