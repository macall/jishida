.mc-text {
display: inline-block;
color: #f49231;
padding: .6em 0;
width: 100%;
}
.mc-text:after {
content: "";
display: inline-block;
border: 0 solid #f49231;
border-width: 0 0 2px 2px;
width: 8px;
height: 8px;
-webkit-transform: rotate(-45deg);
-moz-transform: rotate(-45deg);
-ms-transform: rotate(-45deg);
transform: rotate(-45deg);
position: relative;
margin-left: 3px;
top: -2px;
}

